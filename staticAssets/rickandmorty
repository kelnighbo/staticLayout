if (window.NodeList && !NodeList.prototype.forEach) {
    NodeList.prototype.forEach = function (callback, thisArg) {
        thisArg = thisArg || window;
        for (var i = 0; i < this.length; i++) {
            callback.call(thisArg, this[i], i, this);
        }
    };
}
</script><script class="jsx-4225047008 jsx-275811719 jsx-1139607972 jsx-2138055264">
(function() {
  var cmp = document.cookie.replace(/(?:(?:^|.*;\s*)_hulu_plus_cmp\s*\=\s*([^;]*).*$)|^.*$/, "$1");
  var cmpUrl = document.cookie.replace(/(?:(?:^|.*;\s*)_hulu_plus_cmp_url\s*\=\s*([^;]*).*$)|^.*$/, "$1");
  if (!cmp) {
    var queries;
    try {
      queries = JSON.parse('{"' + decodeURI(location.search.substring(1).replace(/&/g, "\",\"").replace(/=/g, "\":\"")) + '"}');
    } catch(error) {
      queries = null;
    }
    if (queries && queries.cmp) {
      cmp = queries.cmp;
      cmpUrl = window.location.toString();
    }
  }
  // If the cmp url contains wrong hostname, correct it
  if (cmpUrl && !unescape(cmpUrl).includes(window.location.origin)) {
    var url = new URL(unescape(cmpUrl));
    url.protocol = window.location.protocol;
    url.hostname = window.location.hostname;
    cmpUrl = url.toString();
  }
  var date = new Date();
  date.setTime(date.getTime() + 30 * 24 * 60 * 60 * 1000);
  var expiration = date.toUTCString();
  var parts = window.location.hostname.split('.');
  var domain = "." + [parts[parts.length - 2], parts[parts.length - 1]].join('.');
  var path = '/';
  if (cmp.length) {
    document.cookie = "_hulu_plus_cmp=" + cmp + ";domain=" + domain + ";path=" + path + ";expires=" + expiration;
  }
  if (cmpUrl.length) {
    document.cookie = "_hulu_plus_cmp_url=" + cmpUrl + ";domain=" + domain + ";path=" + path + ";expires=" + expiration;
  }
})();
